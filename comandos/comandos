##Compilar com classpath
javac -d target -cp target
./src/main/java/br/com/alura/modelo/dominio/Categoria.java
./src/main/java/br/com/alura/modelo/dominio/Categoria.java 
./src/main/java/br/com/alura/modelo/dominio/Livro.java 
./src/main/java/br/com/alura/modelo/dominio/NotaFiscal.java 
./src/main/java/br/com/alura/http/dao/LivroDAO.java
./src/main/java/br/com/alura/nf/servico/NotaFiscalServico.java
./src/main/java/br/com/alura/nf/subscriber/NotaFiscalSubscriber.java
./src/main/java/br/com/alura/nf/util/NotaFiscalUtil.java
./src/main/java/br/com/alura/Principal.java

##Executar com classpath 
java -cp target br.com.alura.Principal

##Compilar com module path
javac -d mods\br.com.alura --module-path mods 
src\br.com.alura\br\com\alura\modelo\dominio\Categoria.java 
src\br.com.alura\br\com\alura\modelo\dominio\Livro.java 
src\br.com.alura\br\com\alura\modelo\dominio\NotaFiscal.java 
src\br.com.alura\br\com\alura\http\dao\LivroDAO.java
src\br.com.alura\br\com\alura\nf\servico\NotaFiscalServico.java
src\br.com.alura\br\com\alura\nf\subscriber\NotaFiscalSubscriber.java
src\br.com.alura\br\com\alura\nf\util\NotaFiscalUtil.java
src\br.com.alura\br\com\alura\Principal.java

##Executar com module path
java --module-path mods -m br.com.alura/br.com.alura.Principal
